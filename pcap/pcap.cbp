			<Clean command="$make -C $(TARGET_OUTPUT_DIR) -f $makefile distclean" />
			<DistClean command="$make -f $makefile distclean$target" />
			<AskRebuildNeeded command="$make -C $(TARGET_OUTPUT_DIR) -q -f $makefile shared" />
			<SilentBuild command="$make -C $(TARGET_OUTPUT_DIR) -f $makefile shared &gt; $(CMD_NULL)" />
		</MakeCommands>
		<Build>
